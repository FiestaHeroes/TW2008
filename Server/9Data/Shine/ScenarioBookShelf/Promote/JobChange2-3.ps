open [WaitMinuteRoutine]
    interruptset Minute "" 1 WaitMinute "TimeEnd".

    infinite
    open
        waitinterrupt InterruptBlock "InterruptArg".
        call InterruptBlock.
    close
close

open [TimeEnd]
    break "WaitMinuteRoutine".
